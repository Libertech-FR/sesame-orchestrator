{
  "name": "app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "schematics:inherit": "nest generate -c @kradihsoy/lt-schematics inherit",
    "generate:doc": "npx @compodoc/compodoc -p tsconfig.json -d docs --includes ./docs -n 'Sesame Orchestrator'"
  },
  "dependencies": {
    "@kradihsoy/lt-schematics": "^1.0.13",
    "@nestjs-modules/ioredis": "^1.0.1",
    "@nestjs/bullmq": "^10.0.1",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.1.3",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/mongoose": "^10.0.2",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@streamkits/nestjs_module_scrud": "^0.0.16",
    "bullmq": "^4.14.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "ioredis": "^5.3.2",
    "loglevel": "^1.8.1",
    "mongoose": "^8.0.2",
    "nestjs-request-context": "^3.0.0",
    "passport": "^0.6.0",
    "passport-headerapikey": "^1.2.2",
    "radash": "^11.0.0",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.1",
    "yaml": "^2.3.4",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.23",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/testing": "^10.1.3",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.99",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.0.0",
    "@types/passport": "^1.0.15",
    "@types/passport-http": "^0.3.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "mockingoose": "^2.16.2",
    "mongodb-memory-server": "^9.1.3",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1",
      "^~/(.*)$": "<rootDir>/$1"
    }
  },
  "config": {
    "mongodbMemoryServer": {
      "downloadUrl": "https://fastdl.mongodb.org/linux/mongodb-linux-x86_64-debian10-5.0.22.tgz"
    }
  }
}